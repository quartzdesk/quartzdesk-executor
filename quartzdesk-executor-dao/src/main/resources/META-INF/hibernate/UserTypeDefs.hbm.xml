<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

  <!-- ### Start of common types ### -->

  <typedef name="QD_BLOB"
           class="com.quartzdesk.executor.dao.hibernate.usertype.basic.BinaryUT"/>

  <typedef name="QD_BOOLEAN"
           class="com.quartzdesk.executor.dao.hibernate.usertype.basic.BooleanIntegerUT"/>

  <typedef name="QD_CALENDAR"
           class="com.quartzdesk.executor.dao.hibernate.usertype.basic.CalendarUT"/>

  <typedef name="QD_CLOB"
           class="com.quartzdesk.executor.dao.hibernate.usertype.basic.StringUT"/>

  <typedef name="QD_DATE"
           class="com.quartzdesk.executor.dao.hibernate.usertype.basic.DateUT"/>

  <typedef name="QD_DOUBLE"
           class="com.quartzdesk.executor.dao.hibernate.usertype.basic.DoubleUT"/>

  <typedef name="QD_INTEGER"
           class="com.quartzdesk.executor.dao.hibernate.usertype.basic.IntegerUT"/>

  <typedef name="QD_LONG"
           class="com.quartzdesk.executor.dao.hibernate.usertype.basic.LongUT"/>

  <typedef name="QD_SHORT"
           class="com.quartzdesk.executor.dao.hibernate.usertype.basic.ShortUT"/>

  <typedef name="QD_STRING"
           class="com.quartzdesk.executor.dao.hibernate.usertype.basic.StringUT"/>

  <!-- ### End of common types ### -->
</hibernate-mapping>
